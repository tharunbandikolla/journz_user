{
  "hosting": {
    "public": "build/web",
    "appAssociation": "AUTO",
    "ignore": [
      "firebase.json",
      "*/.",
      "*/node_modules/*"
    ],
    "rewrites": [
      {
        "source": "/**",
        "dynamicLinks": true
      },
      
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}